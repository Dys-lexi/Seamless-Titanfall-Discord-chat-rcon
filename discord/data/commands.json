    "botcommands": {
        "rcon": {
            "description": "Send an RCON command to a server",
            "parameters": [
                {
                    "name": "command",
                    "type": "str",
                    "description": "The command to send",
                    "required": true
                }
            ],
            "rcon": true,
            "regularconsolecommand": true
        },
        "getuid": {
            "description": "Get a player's UID",
            "parameters": [
                {
                    "name": "playername",
                    "type": "str",
                    "description": "Get a player's UID",
                    "required": true,
                    "autocompletefunc": "autocompletenamesfromingamenowildcard"
                }
            ],
            "rcon": false,
            "outputfunc": false
        },
        "playing": {
            "description": "List players on a server",
            "outputfunc": "listplayersoverride"
        },
        "throw": {
            "description": "Throw a player",
            "parameters": [
                {
                    "name": "playername",
                    "type": "str",
                    "description": "The player name",
                    "required": true,
                    "autocompletefunc": "autocompletenamesfromingame"
                }
            ],
            "rcon": true,
            "outputfunc": false
        },
        "hostiletf": {
            "description": "Play hostile titanfall warning for a player",
            "parameters": [
                {
                    "name": "playername",
                    "type": "str",
                    "description": "The player name",
                    "required": true,
                    "autocompletefunc": "autocompletenamesfromingame"
                }
            ],
            "rcon": true,
            "outputfunc": false
        },
        "simplesay": {
            "description": "Say stuff in plain text",
            "parameters": [
                {
                    "name": "message",
                    "type": "str",
                    "description": "The message to send",
                    "required": true
                }
            ],
            "rcon": true,
            "outputfunc": false
        },
        "kick": {
            "description": "Kick a player",
            "parameters": [
                {
                    "name": "playername",
                    "type": "str",
                    "description": "The player name",
                    "required": true,
                    "autocompletefunc": "autocompletenamesfromingamenowildcard"
                },
                {
                    "name": "reason",
                    "type": "str",
                    "description": "The reason for the kick",
                    "required": false
                }
            ],
            "rcon": true,
            "outputfunc": false
        },
        "impersonate": {
            "description": "impersonate a player (you meanie)",
            "parameters": [
                {
                    "name": "playername",
                    "type": "str",
                    "description": "Who?",
                    "required": true,
                    "autocompletefunc": "autocompletenamesfromingamenowildcard"
                },
                {
                    "name": "message",
                    "type": "str",
                    "description": "What do you want to say",
                    "required": true
                }
            ],
            "rcon": true,
            "outputfunc": false
        },
        "getconvar": {
            "description": "Get a convar value",
            "parameters": [
                {
                    "name": "convarname",
                    "type": "str",
                    "description": "The convar name",
                    "required": true
                }
            ],
            "rcon": true
        },
        "sanctionban": {
            "description": "Sanction a player",
            "parameters": [
                {
                    "name": "playername",
                    "type": "str",
                    "description": "The player name",
                    "required": true,
                    "autocompletefunc": "autocompletenamesfromingamenowildcard"
                },
                {
                    "name": "reason",
                    "type": "str",
                    "description": "The reason for the sanction",
                    "required": true
                },
                {
                    "name": "sanctiontype",
                    "type": "str",
                    "description": "The type of sanction to apply",
                    "required": true,
                    "choices": [
                        "mute",
                        "ban"
                    ]
                },
                {
                    "name": "expiry",
                    "type": "str",
                    "description": "The expiry time of the sanction in format yyyy-mm-dd, omit is forever",
                    "required": false
                }
            ],
            "rcon": true,
            "commandparaminputoverride": {
                "reason": "-reason",
                "sanctiontype": "-type",
                "expiry": "-expire",
                "appendtoend": "'-issuer '+ctx.author.name"
            },
            "outputfunc": "sanctionoverride"
        },
        "sanctionbanremove": {
            "description": "Remove a sanction",
            "parameters": [
                {
                    "name": "typeofremove",
                    "type": "str",
                    "description": "uid or name",
                    "required": true,
                    "choices": [
                        "uid",
                        "name"
                    ]
                },
                {
                    "name": "who",
                    "type": "str",
                    "description": "Who is unbanned",
                    "required": true
                }
            ],
            "rcon": true,
            "outputfunc": "sanctionoverride"
        },
        "toggleadmin": {
            "description": "Toggle admin status for a player",
            "parameters": [
                {
                    "name": "playername",
                    "type": "str",
                    "description": "The player name",
                    "required": true,
                    "autocompletefunc": "autocompletenamesfromingamenowildcard"
                },
                {
                    "name": "level",
                    "type": "int",
                    "description": "The admin level to set defaults 1",
                    "required": false
                }
            ],
            "rcon": true,
            "outputfunc": false
        }
    }
}